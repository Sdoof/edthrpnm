#thePosition<-position

thePosition$Date<- "2015/9/14"
thePosition$Price<-c(28.2,29.4)
thePosition$UDLY<-1140.8

thePosition$OrigIV<-set.IVOrig(xT=thePosition)
tmp<-set.EuropeanOptionValueGreeks(thePosition)
thePosition$Price<-tmp$Price
thePosition$Delta<-tmp$Delta
thePosition$Gamma<-tmp$Gamma
thePosition$Vega<-tmp$Vega
thePosition$Theta<-tmp$Theta
thePosition$Rho<-tmp$Rho
rm(tmp)
